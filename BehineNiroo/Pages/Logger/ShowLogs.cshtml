                    //}],
                    //"stockLegend": {
                    //    "periodValueTextRegular": "[[value.close]]"
                    //}
                    //}
                ],

                "chartScrollbarSettings": {
                    "graph": "g1",
                    "usePeriod": "10mm",
                },

                "chartCursorSettings": {
                    "valueBalloonsEnabled": true,
                    "fullWidth": true,
                    "cursorAlpha": 0.1,
                    "valueLineBalloonEnabled": true,
                    "valueLineEnabled": true,
                    "valueLineAlpha": 0.5
                },
                "periodSelector": {
                    "position": "left",
                    "dateFormat": "YYYY-MM-DD JJ:NN:SS",
