                        "count": 12,
                        "label": "12 hours"
                    }, {
                        "period": "MAX",
                        "selected": true,
                        "label": "MAX"
                    }]
                },
                "panelsSettings": {
                    "usePrefixes": true
                },
                "dataSetSelector": {
                    "position": "left"
                },
                "export": {
                    "enabled": true
                }
            });

        }

